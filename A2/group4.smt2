(declare-const x1x2 Bool)
(declare-const x1x3 Bool)
(declare-const x1x4 Bool)
(declare-const x2x1 Bool)
(declare-const x2x3 Bool)
(declare-const x2x4 Bool)
(declare-const x3x1 Bool)
(declare-const x3x2 Bool)
(declare-const x3x4 Bool)
(declare-const x4x1 Bool)
(declare-const x4x2 Bool)
(declare-const x4x3 Bool)

(assert  (or (and (not x1x2) (not x1x3) x1x4) (and (not x1x4) (not x1x3) x1x2) (and (not x1x2) (not x1x4) x1x3)))
(assert  (or (and (not x2x1) (not x2x3) x2x4) (and (not x2x4) (not x2x3) x2x1) (and (not x2x1) (not x2x4) x2x3)))
(assert  (or (and (not x3x1) (not x3x2) x3x4) (and (not x3x4) (not x3x2) x3x1) (and (not x3x1) (not x3x4) x3x2)))
(assert  (or (and (not x4x1) (not x4x2) x4x3) (and (not x4x3) (not x4x2) x4x1) (and (not x4x1) (not x4x3) x4x2)))

(assert (not (xor x1x2 x2x1)))
(assert (not (xor x1x3 x3x1)))
(assert (not (xor x1x4 x4x1)))
(assert (not (xor x2x3 x3x2)))
(assert (not (xor x2x4 x4x2)))
(assert (not (xor x3x4 x4x3)))

(assert-soft x1x3)
(assert-soft x1x4)
(assert-soft x2x1)
(assert-soft x2x3)
(assert-soft x2x4)
(assert-soft x3x1)
(assert-soft x4x2)

(check-sat)
(get-model)
(get-objectives)
